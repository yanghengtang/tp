@startuml
!include ./style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":MediConnectParser" as MediConnectParser LOGIC_COLOR
participant ":EditAppointmentCommandParser" as EditAppointmentCommandParser LOGIC_COLOR
participant "e:EditAppointmentCommand" as EditAppointmentCommand LOGIC_COLOR
participant "c:CommandResult" as CommandResult LOGIC_COLOR
participant "d:EditAppointmentDescriptor" as EditAppointmentDescriptor LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("edit_a 5 pic\T01234567B")
activate LogicManager

LogicManager -> MediConnectParser : parseCommand("edit_a 5 pic\T01234567B")
activate MediConnectParser

create EditAppointmentCommandParser
MediConnectParser -> EditAppointmentCommandParser
activate EditAppointmentCommandParser

EditAppointmentCommandParser --> MediConnectParser
deactivate EditAppointmentCommandParser

MediConnectParser -> EditAppointmentCommandParser : parseCommand("edit_a 5 pic\T01234567B")
activate EditAppointmentCommandParser

create EditAppointmentDescriptor
EditAppointmentCommandParser -> EditAppointmentDescriptor
activate EditAppointmentDescriptor

EditAppointmentDescriptor --> EditAppointmentCommandParser : d
deactivate EditAppointmentDescriptor

create EditAppointmentCommand
EditAppointmentCommandParser -> EditAppointmentCommand : EditAppointmentCommand(5, d)
activate EditAppointmentCommand

EditAppointmentCommand --> EditAppointmentCommandParser : e
deactivate EditAppointmentCommand

EditAppointmentCommandParser --> MediConnectParser : e
deactivate EditAppointmentCommandParser

EditAppointmentCommandParser -[hidden]-> MediConnectParser
destroy EditAppointmentCommandParser

MediConnectParser --> LogicManager : e
deactivate MediConnectParser

LogicManager -> EditAppointmentCommand : execute()
activate EditAppointmentCommand

EditAppointmentCommand -> EditAppointmentCommand : createEditedAppointment(5, d)
activate EditAppointmentCommand

EditAppointmentCommand --> EditAppointmentCommand : editedAppointment
deactivate EditAppointmentCommand

EditAppointmentCommand -> Model : setAppointment(5, editedAppointment)
activate Model

Model --> EditAppointmentCommand
deactivate Model

create CommandResult
EditAppointmentCommand -> CommandResult
activate CommandResult

CommandResult --> EditAppointmentCommand : c
deactivate CommandResult

EditAppointmentCommand --> LogicManager : c
deactivate EditAppointmentCommand
@enduml
